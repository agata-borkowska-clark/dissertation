THEORY asubseta IS
     btrue
     =>
     a : FIN(a)
END

&

THEORY asubseta2 IS
    card(a) : NAT
    =>
    a : FIN(a)
END

&


THEORY finfixp IS
    FIN(a) : FIN(FIN(a))
    =>
    a : FIN(a)
END

&

THEORY finfixprev IS
    a : FIN(a)
    =>
    FIN(a) : FIN(FIN(a))
END